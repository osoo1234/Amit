      }
    },
    {
      "cell_type": "code",
      "source": [
        "def prelu(x, alpha=0.01):\n",
        "  return np.where(x > 0, x, alpha * x)\n",
        "x = np.linspace(-10,10,100,1000)\n",
        "z = prelu(x)\n",
        "plt.plot(x,z)\n",
        "plt.xlabel(\"X\")\n",
        "plt.ylabel(\"PReLU(X)\")\n",
        "plt.show()"

      "source": [
        "# Leacky Relu"
      ],
      "metadata": {
        "id": "xKBfN7GYV1Dy"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def leacky_relu(x , alpha=0.01):\n",
        "  return np.where(x > 0, x, alpha * x)\n",
        "x = np.linspace(-10,10,100,1000)\n",
        "z = leacky_relu(x)\n",
        "plt.plot(x,z)\n",
        "plt.xlabel(\"X\")\n",
        "plt.ylabel(\"Leaky Relu(X)\")\n",
        "plt.show()"
      ],
      "metadata": {

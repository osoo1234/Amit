        "# Elu"
      ],
      "metadata": {
        "id": "3TReaBEjV7TQ"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def elu(x , alpha = 1):\n",
        "  return np.where(x > 0, x, alpha * (np.exp(x) - 1))\n",
        "x = np.linspace(-10,10,100,1000)\n",
        "z = elu(x)\n",
        "plt.plot(x,z)\n",
        "plt.xlabel(\"X\")\n",
        "plt.ylabel(\"ELU(X)\")\n",
        "plt.show()"
      ],

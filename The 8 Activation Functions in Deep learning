    {
      "cell_type": "code",
      "source": [
        "def relu(x):\n",
        "  return np.maximum(0,x)\n",
        "x = np.linspace(-10,10,100,1000)\n",
        "z = relu(x)\n",
        "plt.plot(x,z)\n",
        "plt.xlabel(\"X\")\n",
        "plt.ylabel(\"Relu(X)\")\n",
        "plt.show()"

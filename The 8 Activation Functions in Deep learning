        "# Another way to write Swish finction"
      ],
      "metadata": {
        "id": "cLDY_FcQWoUm"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def swish(x):\n",
        "  return x * sigmoid(x)\n",
        "x = np.linspace(-10,10,100,1000)\n",
        "z = swish(x)\n",
        "plt.plot(x,z)\n",
        "plt.xlabel(\"X\")\n",
        "plt.ylabel(\"Swish(X)\")\n",
        "plt.show()"
